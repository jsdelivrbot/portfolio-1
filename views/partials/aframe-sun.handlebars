<a-scene class="aframe sun fadein slow" embedded vr-mode-ui="enabled: false"
  fog="type: linear; color: rgb(138, 30, 46); near: 1; far: 60;">
<!-- #8ab0c6 17, 32, 70-->
  <a-assets>
    <a-asset-item id="terrain1" src="../obj/sun/terrain1.obj"></a-asset-item>
    <a-asset-item id="terrain2-1" src="../obj/sun/terrain2-1.obj"></a-asset-item>
    <a-asset-item id="terrain2-2" src="../obj/sun/terrain2-2.obj"></a-asset-item>
    <a-asset-item id="terrain3" src="../obj/sun/terrain3.obj"></a-asset-item>
    <a-asset-item id="sun" src="../obj/sun/sun.obj"></a-asset-item>
    <a-asset-item id="ship" src="../obj/sun/ship2.obj"></a-asset-item>
    <img id="sun-ring1" src="../obj/sun/sun-ring1.png">
    <img id="sun-ring2" src="../obj/sun/sun-ring2.png">
    <img id="sun-ring3" src="../obj/sun/sun-ring3.png">
    <img id="sun-ring4" src="../obj/sun/sun-ring4.png">
    <img id="sun-ring5" src="../obj/sun/sun-ring5.png">
    <img id="sun-line1" src="../obj/sun/sun-line1.png">
    <img id="sun-line2" src="../obj/sun/sun-line2.png">
    <img id="sun-line3" src="../obj/sun/sun-line3.png">
    <img id="sun-line4" src="../obj/sun/sun-line4.png">
    <img id="sun-line5" src="../obj/sun/sun-line5.png">
    <img id="sun-line6" src="../obj/sun/sun-line6.png">
    <img id="sun-line7" src="../obj/sun/sun-line7.png">
    <img id="sun-line8" src="../obj/sun/sun-line8.png">
    <img id="sun-line9" src="../obj/sun/sun-line9.png">
    <img id="t-tex1" src="../obj/sun/t-tex1.png">

  </a-assets>

  <a-entity id="terrain" position="0 -1 0">
    <a-entity obj-model="obj: #terrain1" src="#t-tex1" position="3.40 -.2 -5.25" rotation="-5.06 0 0"></a-entity>
    <a-entity obj-model="obj: #terrain2-1" position="2.00 -2 -5.00" scale="5 3 3"></a-entity>
    <a-entity obj-model="obj: #terrain2-2" position="50 -3 -20" scale="4 6 4"></a-entity>
    <a-entity obj-model="obj: #terrain3" position="0 -2 -10" scale="4 6 3"></a-entity>
    <a-entity obj-model="obj: #terrain3" position="-2 -4 -5" scale="9 5 3" rotation="0 90 0"></a-entity>
  </a-entity>

  <a-entity id="ships" position="-70 10 -50">
    <a-entity obj-model="obj: #ship" scale="0.5 0.5 0.5"
      position="-90 3 0"><a-animation attribute="position"
        delay="2000"
        dur="500"
        to="20 3 0"
        from="-90 3 0"
        repeat="0"
        easing="linear"
      </a-animation></a-entity>
    <a-entity obj-model="obj: #ship" scale="0.2 0.2 0.2"
      position="-100 9 0">
      <a-animation attribute="position"
        delay="2500"
        dur="500"
        to="35 9 0"
        from="-100 9 0"
        repeat="0"
        easing="linear"
      </a-animation></a-entity>
    <a-entity obj-model="obj: #ship" scale="0.15 0.15 0.15"
      position="-150 0 0">
      <a-animation attribute="position"
        delay="1300"
        dur="500"
        to="25 0 0"
        from="-150 0 0"
        repeat="0"
        easing="linear"
      </a-animation></a-entity>
    <a-entity obj-model="obj: #ship" scale="0.2 0.2 0.2"
      position="-70 15 0">
      <a-animation attribute="position"
        delay="1800"
        dur="500"
        to="10 15 0"
        from="-70 15 0"
        repeat="0"
        easing="linear"
      </a-animation></a-entity>
    <a-entity obj-model="obj: #ship" scale="1 1 1"
      position="40 10 0" rotation="180 45 90"><a-animation attribute="position"
        dur="70000"
        to="60 50 0"
        from="60 10 0"
        repeat="0"
        easing="linear"
      </a-animation>
    </a-entity>
    <!-- <a-entity obj-model="obj: #ship" scale="2.5 2.5 2.5"
      position="20 -3 3" rotation="0 0 0"></a-entity> -->
  </a-entity>

  {{> aframe-sun-rings}}
  <a-entity light="type: point; color: #b83e5e; intensity: 1; distance: 60;"  position="30 40 -20"></a-entity>
  <a-entity light="type: point; color: #ff3e32; intensity: 1; distance: 60;"  position="30 40 -20"></a-entity>
  <a-light type="ambient" color="#f6e799" intensity=".1"></a-light>
  <a-entity camera position="0 0 0">
    <a-animation attribute="position" to="5 0 0" from="0 0 0" dur="30000" easing="linear" repeat="indefinite" direction="alternate"></a-animation></a-entity>
  <a-gradient-sky material="shader: gradient; topColor: 17, 32, 70; bottomColor: 187, 46, 67;"></a-gradient-sky>

</a-scene>
