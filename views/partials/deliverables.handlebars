<div class="row">
  <h2 class="tc">Project Deliverables</h2>
  <div class="col-md-12">
    <div class="row">
      {{#each deliverablesrow1}}
      <div class="col-md-3">
        <div class="row">
          <div class="col-md-6">
            <img src="{{this.[0]}}" class="img-grid" alt="{{this.[1]}}" />
          </div>
          <div class="col-md-6">
            <h3>{{this.[1]}}</h3>
            <h4>{{this.[2]}}</h4>
          </div>
        </div>
      </div>
      {{/each}}
    </div>
    <div class="row">
      {{#each deliverablesrow2}}
      <div class="col-md-3">
        <div class="row">
          <div class="col-md-6">
            <img src="{{this.[0]}}" class="img-grid" alt="{{this.[1]}}" />
          </div>
          <div class="col-md-6">
            <h3>{{this.[1]}}</h3>
            <h4>{{this.[2]}}</h4>
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
</div>
